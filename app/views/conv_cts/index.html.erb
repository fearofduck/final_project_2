<div class="page-header">
  <h1>
    Conv cts
    <a href="/conv_cts/new" class="btn btn-lg btn-success">New Conv ct</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Jan</th>
        <th>Feb</th>
        <th>Mar</th>
        <th>Apr</th>
        <th>May</th>
        <th>Jun</th>
        <th>Jul</th>
        <th>Aug</th>
        <th>Sep</th>
        <th>Oct</th>
        <th>Nov</th>
        <th>Dec</th>
        <th>Name</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @conv_cts.each do |conv_ct| %>
      <tr>
        <td><%= conv_ct.jan %></td>
        <td><%= conv_ct.feb %></td>
        <td><%= conv_ct.mar %></td>
        <td><%= conv_ct.apr %></td>
        <td><%= conv_ct.may %></td>
        <td><%= conv_ct.jun %></td>
        <td><%= conv_ct.jul %></td>
        <td><%= conv_ct.aug %></td>
        <td><%= conv_ct.sep %></td>
        <td><%= conv_ct.oct %></td>
        <td><%= conv_ct.nov %></td>
        <td><%= conv_ct.dec %></td>
        <td><%= conv_ct.name %></td>
        <td><%= conv_ct.user_id %></td>
        <td>
          <a href="/conv_cts/<%= conv_ct.id %>" class="btn btn-primary">Show</a>
          <a href="/conv_cts/<%= conv_ct.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_conv_ct/<%= conv_ct.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
