<div class="page-header">
  <h1>
    Ds40 cts
    <a href="/ds40_cts/new" class="btn btn-lg btn-success">New Ds40 ct</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Jan</th>
        <th>Feb</th>
        <th>Mar</th>
        <th>Apr</th>
        <th>May</th>
        <th>Jun</th>
        <th>Jul</th>
        <th>Aug</th>
        <th>Sep</th>
        <th>Oct</th>
        <th>Nov</th>
        <th>Dec</th>
        <th>Name</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @ds40_cts.each do |ds40_ct| %>
      <tr>
        <td><%= ds40_ct.jan %></td>
        <td><%= ds40_ct.feb %></td>
        <td><%= ds40_ct.mar %></td>
        <td><%= ds40_ct.apr %></td>
        <td><%= ds40_ct.may %></td>
        <td><%= ds40_ct.jun %></td>
        <td><%= ds40_ct.jul %></td>
        <td><%= ds40_ct.aug %></td>
        <td><%= ds40_ct.sep %></td>
        <td><%= ds40_ct.oct %></td>
        <td><%= ds40_ct.nov %></td>
        <td><%= ds40_ct.dec %></td>
        <td><%= ds40_ct.name %></td>
        <td>
          <% if ds40_ct.user.present? %>
            <a href="/users/<%= ds40_ct.user.id %>">
              <%= ds40_ct.user.email %>
            </a>
          <% end %>
        </td>
        <td>
          <a href="/ds40_cts/<%= ds40_ct.id %>" class="btn btn-primary">Show</a>
          <a href="/ds40_cts/<%= ds40_ct.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_ds40_ct/<%= ds40_ct.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @ds40_cts, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
