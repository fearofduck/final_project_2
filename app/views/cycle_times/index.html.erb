<div class="page-header">
  <h1>
    Cycle times
    <a href="/cycle_times/new" class="btn btn-lg btn-success">New Cycle time</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Scenario</th>
        <th>Ds40 ct</th>
        <th>Conv ct</th>
        <th>Ds53 ct</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @cycle_times.each do |cycle_time| %>
      <tr>
        <td><%= cycle_time.scenario %></td>
        <td><%= cycle_time.ds40_ct.jan %></td>
        <td><%= cycle_time.conv_ct.jan %></td>
        <td><%= cycle_time.ds53_ct.jan %></td>
        <td><%= cycle_time.user_id %></td>
        <td>
          <a href="/cycle_times/<%= cycle_time.id %>" class="btn btn-primary">Show</a>
          <a href="/cycle_times/<%= cycle_time.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_cycle_time/<%= cycle_time.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
